{
  "name": "downstream-electron",
  "version": "0.1.1",
  "description": "DOWNstream For Electron",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://castlabs.com/open-source/downstream/",
  "scripts": {
    "build": "webpack",
    "eslint": "./node_modules/.bin/eslint api",
    "jsdoc": "./node_modules/.bin/jsdoc api -r --readme api/README.md --configure jsdoc.conf.json -d jsdoc",
    "jsdoc_prv": "./node_modules/.bin/jsdoc api -r -p --readme api/README.md --configure jsdoc.conf.json -d jsdoc",
    "jasmine": "(npm run jasmine_test || true) && npm run jasmine_report",
    "jasmine_test": "./node_modules/jasmine-node/bin/jasmine-node units/ --junitreport --output tmp",
    "jasmine_report": "./node_modules/junit-viewer/bin/junit-viewer --results=tmp/TEST-utildownloadsjs.xml --save=tmp/units_result.html",
    "start": "electron .",
    "travis-build": "npm run eslint && npm run jasmine_test && npm run build && npm run jsdoc && true"
  },
  "author": "castlabs GmbH",
  "maintainers": [
    {
      "name": "Bartlomiej Obecny",
      "email": "bobecny@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "clean-webpack-plugin": "0.1.17",
    "eslint": "4.8.0",
    "jasmine-node": "^1.14.5",
    "jquery": "3.2.1",
    "jsdoc": "3.5.5",
    "jsdoc-webpack-plugin": "0.0.2",
    "junit-viewer": "^4.11.1",
    "shaka-player": "2.2.2",
    "webpack": "3.6.0"
  },
  "dependencies": {
    "biguint-format": "1.0.0",
    "cors": "2.8.4",
    "express": "4.16.2",
    "flake-idgen": "1.1.0",
    "fs-extra": "4.0.2",
    "jsonfile": "4.0.0",
    "electron": "https://github.com/castlabs/electron-releases#v1.8.1-vmp1010",
    "mkdirp": "0.5.1",
    "moment": "2.19.0",
    "request": "https://github.com/castlabs/request",
    "underscore": "1.8.3",
    "url-parse": "1.1.9",
    "xmldom": "0.1.27"
  }
}
